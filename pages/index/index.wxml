            <text class="text4">单</text>
          </view>
        </view>
       
      </view>
      <view class="box box1 pl-38 bga8">

        <view class="text1">VISIT TODAY</view>
        <view class="text2">累计会员</view>
      </view>
      <view class="box box2 bga9">

        <text class="text3">3250</text>
        <text class="text4">辆</text>
      </view>
      <view class="box box1 pl-38 box-bg">
        <view class="text1">ERSHOUCHE</view>
        <view class="text2">二手车交易</view>
      </view>
    </view>
  </view>
</view>
